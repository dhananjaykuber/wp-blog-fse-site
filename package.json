	"repository": {
		"type": "git",
		"url": "https://github.com/dhananjaykuber/starter-block-theme"
	},
	"bugs": {
		"url": "https://github.com/dhananjaykuber/starter-block-theme/issues"
	},
	"devDependencies": {
		"@wordpress/scripts": "^30.9.0",
		"clean-webpack-plugin": "^4.0.0",
		"glob": "^11.0.1"
	}
}
